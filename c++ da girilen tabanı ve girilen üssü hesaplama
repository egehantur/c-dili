#include <stdio.h>
int sayi(int,int);
int main(){
	int x;
	int y;
	printf("Üssü giriniz : ");
	scanf("%d",&x);
	printf("Tabanı giriniz : ");
	scanf("%d",&x);
}
int sayi(int a,int b){
	if (a == 0 && b == 0){
		printf("Tanımsız");
	}
	else if (a == 0 || b == 0){
		return 1;
	}
	else if (b == 1){
		return a;
	}
	else{
		return a**b;
	}
}
