#include <stdio.h>
int main(){
	int a;
	int sayilar[10];
	for(a=0;a<10;a++){
		scanf("½d",&sayilar[a]);
	}
	for(a=0;a<10;a++){
		scanf("%d",&sayilar[a]);
	}
	for(a=0;a<10;a++){
		printf("%d",sayilar[a]);
	}
	int x;
	printf("Kaçıncı sayıyı görmek istiyorsunuz? ");
	scanf("%d",&x);
	printf("%d",sayilar[x]);
}
